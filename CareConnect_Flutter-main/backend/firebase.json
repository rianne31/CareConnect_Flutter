{
  "functions": [
    {
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ],
      "predeploy": [
        
      ],
      "source": "functions",
      "runtime": "nodejs20"
    },
    {
      "codebase": "carecon",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ],
      "predeploy": [
        
      ],
      "source": "carecon"
    }
  ],
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "storage": {
    "rules": "storage.rules"
  },
  "hosting": {
    "public": "../flutter_app/build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "emulators": {
    "hub": {
      "port": 4429
    },
    "ui": {
      "port": 4029
    },
    "firestore": {
      "host": "127.0.0.1",
      "port": 8099
    },
    "functions": {
      "host": "127.0.0.1",
      "port": 5019
    }
  }
}
